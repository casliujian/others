import Dummy
(*datatype t1 = int*)
datatype t2 = int
datatype t3 = {
        x : int;
        y : bool;
    }
datatype day = Monday | Tuesday | Wednesday | Thursday | Friday | Saturday | Sunday

val x = 10
var y = 20
var z = 4

function day_of_z z : int -> day = 
    match z with
    | 1 -> Monday
    | 2 -> Tuesday
    | 3 -> Wednesday
    | 4 -> Thursday
    | 5 -> Friday
    | 6 -> Saturday
    | _ -> Sunday

function is_sunday d : day -> bool= 
    d = Sunday

Model {
    state s = 1
    transition s = 
        if s < 7 then 
            s+1 
        else 
            1
    property dummy = is_sunday (day_of_z s)
    property reach_sunday = AF (x, is_sunday (day_of_z x), s)
}